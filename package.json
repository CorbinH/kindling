{
    "bin": {
        "kindling": "jdeploy-bundle/jdeploy.js"
    },
    "author": "Paul Griffith",
    "description": "A standalone collection of utilities to help Ignition users.",
    "main": "index.js",
    "preferGlobal": true,
    "repository": "",
    "version": "0.0.6",
    "jdeploy": {
        "jdk": false,
        "args": [],
        "javaVersion": "17",
        "documentTypes": [
            {
                "extension": "gwbk",
                "mimetype": "application/gwbk"
            },
            {
                "extension": "idb",
                "mimetype": "application/idb"
            },
            {
                "extension": "log",
                "mimetype": "text/log"
            },
            {
                "extension": "json",
                "mimetype": "application/json"
            }
        ],
        "jar": "build/libs/kindling.jar",
        "javafx": false,
        "title": "Kindling"
    },
    "dependencies": {
        "njre": "^0.2.0",
        "shelljs": "^0.8.4"
    },
    "license": "MIT",
    "name": "ignition-kindling",
    "files": [
        "jdeploy-bundle"
    ],
    "homepage": "https://github.com/paul-griffith/kindling"
}
